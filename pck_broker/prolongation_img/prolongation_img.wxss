page {
    background: #fff;
}

.code-wrap,.prolongation-img {
    padding-bottom: 30rpx;
}

.code-wrap {
    width: 690rpx;
    border-radius: 5px;
    overflow: hidden;
    background: #fff;
    margin: 60rpx auto 0;
    box-shadow: 0 2px 10px 0 hsla(0,0%,78%,.5);
}

.loadbtn-wrap {
    position: relative;
    box-sizing: border-box;
    height: 270rpx;
    background: #f4f4f9;
    padding-top: 85rpx;
}

.z-btn-load {
    width: 290rpx;
    height: 50px;
    line-height: 46px;
    text-align: center;
    padding: 0;
    background: transparent;
    font-size: 15px;
    color: #4a4c5b;
    border: 4rpx dashed #4a4c5b;
}

.z-btn-load.loaded {
    border-style: solid;
    text-align: right;
    padding-right: 50rpx;
}

.right-angle {
    position: absolute;
    width: 26rpx;
    height: 18rpx;
    top: 126rpx;
    left: 260rpx;
}

.add-type-wrap {
    box-sizing: border-box;
    padding: 40rpx 30rpx 0;
}

.type-title {
    font-size: 30rpx;
    line-height: 20px;
    color: #000;
    font-weight: 700;
    margin-bottom: 85rpx;
}

.type-options-list {
    overflow: hidden;
}

.type-options-item {
    float: left;
    width: 50%;
}

.type-options-name {
    font-size: 15px;
    line-height: 20px;
    color: #020202;
    text-align: center;
}

.type-options-checked-icon {
    width: 26rpx;
    height: 16rpx;
    margin: 10rpx auto 15rpx;
}

.type-options-checked-icon image {
    display: block;
    width: 100%;
    height: 100%;
}

.type-options-example {
    width: 211rpx;
    height: 272rpx;
    margin: 0 auto;
    box-sizing: border-box;
}

.type-options-example.checked {
    border: 2px solid #4a4c5b;
    width: 236rpx;
    height: 305rpx;
}

.type-options-example image {
    display: block;
    width: 100%;
    height: 100%;
}

canvas {
    width: 335px;
    margin: 30rpx auto 0;
}

.btn-download {
    position: fixed;
    bottom: 40rpx;
    left: 30rpx;
    width: 690rpx;
    height: 90rpx;
    background: #4a4c5b;
    border-radius: 4px;
    color: #fff;
    line-height: 90rpx;
    text-align: center;
    letter-spacing: 2rpx;
    font-size: 32rpx;
    z-index: 20;
}

.btn-download image {
    display: block;
    width: 100%;
    height: 100%;
}

.z-btn {
    height: 90rpx;
    background: #4a4c5b;
    border-radius: 8rpx;
    font-size: 32rpx;
    color: #fff;
    margin: 0 30rpx;
}

.building-tab {
    padding-top: 70rpx;
    padding-bottom: 50rpx;
}