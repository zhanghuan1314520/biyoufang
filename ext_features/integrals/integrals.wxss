view {
    box-sizing: border-box;
}

.container,page {
    height: 100%;
    width: 100%;
}

.container {
    display: flex;
    flex-direction: column;
    font-family: PingFang-SC-Medium,PingFang-SC;
    font-weight: 500;
}

.head-data {
    background: linear-gradient(1turn,#ff8200,#ffb400);
    color: #fff;
    padding-bottom: 37rpx;
}

.head-data>view {
    width: 50%;
}

.head-num {
    font-size: 70rpx;
    font-family: DIN-Bold,DIN;
    font-weight: 700;
    line-height: 85rpx;
    position: relative;
    margin-top: 50rpx;
    width: 100%;
    text-align: center;
}

.head-text {
    font-size: 24rpx;
    font-weight: 500;
    line-height: 33rpx;
    margin-top: 5rpx;
}

.head-num_bord::before {
    content: "";
    position: absolute;
    width: 3rpx;
    top: 23rpx;
    right: 0;
    border-top: 60rpx solid #fff;
    opacity: .8;
}

.head-title {
    background: #fff9e2;
    padding: 15rpx 30rpx;
    font-size: 24rpx;
    color: #fe5e10;
    line-height: 33rpx;
}

.main {
    overflow-y: auto;
    width: 100%;
    padding: 0 30rpx 49rpx;
}

.main-text {
    font-size: 38rpx;
    font-weight: 500;
    color: #333;
    line-height: 53rpx;
    padding-top: 33rpx;
}

.main-btn {
    width: 144rpx;
    height: 60rpx;
    background: #fff;
    border-radius: 31rpx;
    border: 2rpx solid #fe5e10;
    font-size: 26rpx;
    color: #fe5e10;
    line-height: 60rpx;
    text-align: center;
    margin-left: 120rpx;
    position: relative;
}

.main-title {
    padding-top: 29rpx;
    display: flex;
}

.m_title-expla {
    font-size: 32rpx;
    color: #333;
    line-height: 32rpx;
}

.m_title-item {
    font-size: 22rpx;
    font-weight: 600;
    color: #ff8400;
    line-height: 32rpx;
    margin-left: 20rpx;
}

.m_title-stick {
    font-size: 32rpx;
}

.m_title-limited {
    padding: 24rpx 0;
    font-size: 24rpx;
    font-weight: 400;
    color: #999;
    line-height: 32rpx;
}