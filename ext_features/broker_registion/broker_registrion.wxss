.form-wrap {
    box-sizing: border-box;
}

.form-group {
    display: flex;
    margin: 0 30rpx;
}

.form-group-top {
    border-top: 1rpx solid #ededed;
}

.form-group-normal {
    display: block;
}

.form-group.form-group-normal {
    flex: 1;
}

.flex {
    display: flex;
}

.photo-box {
    padding-bottom: 30rpx;
}

.photo-box>view {
    padding-top: 30rpx;
}

.form-group:not(.form-group-bottom) {
    border-bottom: 1rpx solid #ededed;
}

.form-group-text {
    font-size: 30rpx;
    color: #666;
    margin-right: 16rpx;
}

.form-label {
    flex: none;
    font-size: 30rpx;
    line-height: 88rpx;
    color: #333;
    width: 280rpx;
}

.form-label-left {
    padding-left: 30rpx;
}

.form-input-wrap,.form-input-wrap-outside {
    flex: 1;
}

.form-input-wrap {
    min-height: 88rpx;
    display: flex;
    align-items: center;
    justify-content: start;
}

.input-wrap-with-bg {
    box-sizing: border-box;
    border-radius: 8rpx;
}

.input-wrap-error {
    border: 1px solid #fe4d3d!important;
}

.form-input {
    flex: 1;
    font-size: 30rpx;
}

.phone-box {
    padding: 27rpx 0 17rpx;
}

.form-fake-link,.form-input-fake {
    font-size: 30rpx;
}

.input-place {
    color: #ccc;
}

.form-input-fake {
    color: #ccc;
    max-width: 270rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.form-input-fake-have {
    color: #333;
}

.form-fake-link {
    color: #4b70c4;
    text-align: right;
    font-size: 24rpx;
    color: #ccc;
}

.form-fake-link>image {
    width: 11rpx;
    height: 20rpx;
    margin-right: 15rpx;
    margin-left: 8rpx;
    vertical-align: middle;
}

.form-icon-search {
    flex: none;
    width: 28rpx;
    height: 28rpx;
}

.form-handler,.form-phone {
    font-size: 28rpx;
}

.form-handler {
    color: #999;
    margin-left: 15rpx;
    line-height: 32rpx;
    text-align: left;
}

.btn-handler {
    width: auto;
    padding: 0;
    line-height: 32rpx;
    background: transparent;
    margin-left: 0;
    margin-right: 5rpx;
    border-bottom: 1rpx solid #999;
    margin-bottom: 23rpx;
    border-radius: 0;
}

.form-textarea {
    display: block;
    width: 100%;
    height: 150rpx;
    font-size: 26rpx;
    line-height: 37rpx;
    background-color: #f9f9f9;
    padding: 14rpx 18rpx;
}

.form-img-wrap {
    position: relative;
    width: 140rpx;
    height: 140rpx;
    background: #f9f9f9;
    border-radius: 8rpx;
    overflow: hidden;
}

.form-img {
    display: block;
    width: 100%;
    height: 100%;
    position: relative;
    z-index: 3;
}

.form-img-icon {
    position: absolute;
    z-index: 2;
    left: 50%;
    top: 50%;
    margin: -32rpx 0 0 -32rpx;
    width: 64rpx;
    height: 64rpx;
}

.z-btn-box {
    background-color: #fff;
}

.z-btn {
    background: linear-gradient(270deg,#fe5e10,#ff831c);
    border-radius: 8rpx;
    font-size: 34rpx;
    color: #fff;
    margin-top: 50rpx;
    width: 690rpx;
    margin-bottom: 32rpx;
}

.register__info {
    background: #f5f5f5;
    padding: 30rpx 30rpx 130rpx;
}

.register__state {
    background: #fff;
    box-sizing: border-box;
    padding: 15rpx 30rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-radius: 4rpx;
    margin-bottom: 30rpx;
}

.register__state-icon {
    flex: none;
    width: 110rpx;
    height: 110rpx;
    margin-top: 20rpx;
}

.register__state-text {
    flex: 1;
    font-weight: 600;
    color: #333;
    font-size: 36rpx;
    text-align: center;
    line-height: 80rpx;
}

.register__state-title {
    font-size: 24rpx;
    color: #333;
    line-height: 33rpx;
}

.state-fail {
    text-align: left;
    padding-left: 18rpx;
}

.register__wrap {
    box-sizing: border-box;
    padding: 0 30rpx;
    background: #fff;
}

.register__wrap-bord {
    border-bottom: 1rpx dashed #ededed;
}

.register__img-wrap {
    width: 230rpx;
    height: 230rpx;
    border-radius: 8rpx;
    overflow: hidden;
    border: 4rpx solid #f9f9f9;
    padding: 0 20rpx;
}

.register__img {
    display: block;
    width: 100%;
    height: 100%;
}

.b__cell-content,.b__cell-title {
    padding: 25rpx 0;
}

.b__cell-title {
    width: 80px;
}

.b__cell-content {
    margin-left: 85px;
}

.b__cell-content.with-border {
    border-bottom: 1px solid #eaeaea;
}

.btn-edit {
    position: fixed;
    right: 0;
    left: 0;
    bottom: 0;
    height: 90rpx;
    border-radius: 4rpx;
    background: linear-gradient(270deg,#fe5e10,#ff831c);
    color: #fff;
    font-size: 34rpx;
}

.btn-edit-img {
    display: block;
    width: 32rpx;
    height: 32rpx;
}

.edit-desc {
    font-size: 24rpx;
    line-height: 44rpx;
    color: #fe6011;
}

.flex-between {
    align-items: flex-start;
    flex-direction: column;
}

.form-tip-bar {
    font-size: 24rpx;
    font-weight: 400;
    color: #979797;
    line-height: 33rpx;
    background: #f7f7f7;
    padding: 17rpx 0 17rpx 29rpx;
}

.form-label-sub {
    color: #9aa9b4;
    font-size: 22rpx;
}

.worker-permit {
    padding-bottom: 16rpx;
    color: #fe5e10;
    font-size: 24rpx;
    line-height: 33rpx;
}

.align-between {
    justify-content: space-between;
    padding-right: 15rpx;
}

.form-input-wrap-end {
    flex: 1;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.setted-tips {
    color: #ff7e1a;
    font-size: 22rpx;
    font-weight: 500;
    margin-right: 13rpx;
}