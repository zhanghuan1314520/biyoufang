.reset-btn {
    background-color: #fff;
    margin: 0;
    padding: 0;
    line-height: 1;
    border-radius: 0;
}

.container {
    background-color: #faf3ec;
    min-height: 100vh;
}

.top-bar {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 370rpx;
    background: url("data:image/jpeg;base64,") no-repeat;
    background-size: 100%;
}

.top-bar .total-integral {
    font-size: 22rpx;
    color: #fff;
    position: absolute;
    top: 175rpx;
    left: 50%;
    transform: translateX(-50%);
    font-weight: 700;
    text-align: center;
}

.top-bar .total-integral .integral {
    font-size: 50rpx;
    text-shadow: 0 2rpx 1rpx #f29f2f;
    padding-bottom: 23rpx;
}

.integral-wrap {
    margin: -46rpx 25rpx 0;
    background-color: #fff;
    border-radius: 20rpx;
}

.integral-wrap .integral-item {
    display: flex;
    margin: 0 30rpx;
    padding: 30rpx 0;
    border-bottom: 1px solid #eaeaea;
    min-width: 0;
}

.integral-wrap .integral-item .integral-item-left {
    flex: 1;
    min-width: 0;
}

.integral-wrap .integral-item .integral-item-left .name {
    font-size: 32rpx;
    color: #333;
    font-weight: 500;
    padding-bottom: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.integral-wrap .integral-item .integral-item-left .time {
    font-size: 24rpx;
    color: #999;
    padding-bottom: 10rpx;
}

.integral-wrap .integral-item .integral-item-left .building {
    font-size: 24rpx;
    color: #999;
}

.integral-wrap .integral-item .integral-item-right {
    width: 135rpx;
    text-align: right;
}

.integral-wrap .integral-item .integral-item-right .integral-type {
    font-size: 40rpx;
    color: #ff8400;
    font-weight: 700;
    padding-bottom: 10rpx;
}

.integral-wrap .integral-item .integral-item-right .integral-over {
    font-size: 24rpx;
    color: #999;
}