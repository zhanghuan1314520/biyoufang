.reset-btn {
    background-color: #fff;
    margin: 0;
    padding: 0;
    line-height: 1;
    border-radius: 0;
}

.container {
    padding-top: calc(65px + 86rpx);
    padding-bottom: 130rpx;
    min-height: 100vh;
    min-width: 100vw;
    box-sizing: border-box;
}

.back-top {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    position: fixed;
    bottom: 150rpx;
    right: -90rpx;
}

.back-top .back-top-icon {
    width: 100%;
    height: 100%;
}

.advert-video {
    width: 100%;
    height: 420rpx;
    vertical-align: top;
}

.reserve-form_root {
    padding: 50rpx 60rpx!important;
}