page {
    padding-bottom: 20rpx;
}

.index {
    width: 100%;
    height: 100%;
}

.section-distance {
    border-bottom: 12rpx solid #f7f7f7;
}

.search-wrapper {
    height: 50px;
    position: relative;
}

.building-search {
    position: fixed;
    left: 0;
    right: 0;
    display: flex;
    height: 41px;
    padding: 0rpx 13px 9px 10px;
    background-color: #fe5e10;
    background: linear-gradient(270deg,#fe5e10,#ff7603);
    z-index: 2;
}

.building-search-location,.building-search-share {
    position: relative;
    flex-direction: column;
    justify-content: center;
}

.building-search-location,.building-search-share,.building-search-share__btn {
    display: flex;
    align-items: center;
}

.icon_location {
    width: 40rpx;
    height: 40rpx;
}

.city-info {
    display: flex;
    align-items: center;
    margin-top: 6rpx;
    font-size: 22rpx;
    color: #fff;
    justify-content: space-between;
}

.icon_down {
    margin-left: 10rpx;
    width: 18rpx;
    height: 12rpx;
}

.building-search-input {
    display: flex;
    align-items: center;
    flex: 1;
    margin: 0 15rpx;
    height: 80rpx;
    border: 1px solid #fff;
    border-radius: 8rpx;
    background-color: #fff;
}

.icon_search {
    margin: 0 18rpx;
    width: 28rpx;
    height: 28rpx;
}

.building-search-input__inner {
    font-size: 30rpx;
    color: #ccc;
}

.icon_share {
    width: 40rpx;
    height: 40rpx;
}

.share-info {
    margin-top: 6rpx;
    font-size: 22rpx;
    color: #fff;
}

.swiper-skeleton {
    width: 100%;
    height: 300rpx!important;
}

.swiper {
    position: relative;
    height: 300rpx;
    margin: 30rpx 30rpx 0;
    font-size: 0;
}

.swiper swiper {
    width: 100%;
    height: 100%;
}

.slide-image {
    height: 100%!important;
    border-radius: 8rpx;
}

.data-desc {
    font-size: 11px;
    height: 30rpx;
    line-height: 30rpx;
    color: #020202;
    margin-top: 9rpx;
}

.list {
    padding-bottom: 28rpx;
}

.list:last-child {
    border-bottom: none;
}

.list__title {
    position: relative;
    box-sizing: border-box;
    height: 108rpx;
    line-height: 108rpx;
    padding: 0 30rpx;
}

.list__title-icon {
    position: absolute;
    width: 10rpx;
    height: 30rpx;
    left: 30rpx;
    top: 38rpx;
}

.list__title-text {
    font-size: 19px;
    font-weight: 700;
    color: #333;
}

.list__title-link {
    position: absolute;
    right: 30rpx;
    top: 38rpx;
    height: 30rpx;
    line-height: 30rpx;
    font-size: 12px;
    color: #777;
}

.list__title-link-arrow {
    width: 12rpx;
    height: 24rpx;
}

.headlines-skeleton {
    display: flex;
    padding: 20rpx 30rpx;
}

.headlines-skeleton__left {
    width: 127rpx;
    border-right: 1px solid #ededed;
}

.headlines-skeleton__right {
    flex: 1;
    margin-left: 20rpx;
}

.headRecommend {
    padding-bottom: 2rpx;
}

.headRecommend__inner {
    box-sizing: border-box;
    padding: 6rpx 0;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    margin: 0 30rpx;
    box-shadow: 0 0 12rpx 0 hsla(0,0%,74.5%,.5);
    border-radius: 12rpx;
}

.headRecommend__left {
    line-height: 0;
    padding-left: 19rpx;
}

.headRecommend__icon,.headRecommend__left {
    width: 127rpx;
    height: 29rpx;
}

.headRecommend__line {
    width: 1rpx;
    background: #ededed;
    height: 40rpx;
    margin: 0 29rpx 0 18rpx;
}

.headRecommend__right {
    box-sizing: border-box;
    flex: 1;
    height: 60rpx;
    display: flex;
    align-items: center;
    overflow: hidden;
}

.headRecommend__swiper {
    width: 100%;
    height: 140rpx;
    font-size: 24rpx;
    color: #333;
}

.headRecommend__item {
    display: flex;
    box-sizing: border-box;
    padding: 30rpx 0;
    flex-direction: column;
}

.headRecommend__item,.headRecommend__item-center {
    justify-content: center;
}

.headRecommend__title {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.notice-wrap {
    height: 70rpx;
}

.announcement__wrap {
    position: fixed;
    left: 0;
    right: 0;
    box-sizing: border-box;
    padding: 0rpx 30rpx;
    line-height: 70rpx;
    z-index: 2;
    background-color: #fff9e2;
}

.announcement {
    width: 640rpx;
    font-size: 24rpx;
    color: #fe5e10;
    overflow: hidden;
    white-space: nowrap;
}

.announcement__text {
    display: inline-block;
}

.announcement__icon {
    position: absolute;
    right: 28rpx;
    top: 50%;
    width: 23rpx;
    height: 23rpx;
    transform: translateY(-50%);
}

.addtomymp__wrap {
    position: fixed;
    right: 20rpx;
    padding: 14rpx 28rpx;
    box-sizing: border-box;
    z-index: 11;
    width: 388rpx;
    height: 144rpx;
    background: linear-gradient(1turn,#ffdca2,#ffc749);
    box-shadow: 0 10rpx 18rpx 0 rgba(0,0,0,.5);
    border-radius: 4rpx 0 0 0;
}

.addtomymp__wrap::after {
    content: "";
    position: absolute;
    right: 128rpx;
    top: -32rpx;
    width: 0;
    height: 0;
    border: 16rpx solid transparent;
    border-bottom-color: #fff4e5;
    z-index: 12;
}

.addtomymp {
    font-size: 28rpx;
    color: #6b580a;
    overflow: hidden;
    line-height: 44rpx;
}

.btn_close {
    position: absolute;
    bottom: 12rpx;
    right: 24rpx;
    height: 33rpx;
    font-size: 28rpx;
    color: #333;
    text-decoration: underline;
}

.mask {
    position: fixed;
    top: 130rpx;
    left: 0;
    width: 100%;
    background-color: #414141;
    opacity: .9;
    z-index: 5;
    overflow: hidden;
}

.collect-intro {
    position: absolute;
    top: 0;
    z-index: 10;
}

.collect-intro .collect-intro__btn_desktop {
    width: 224px;
    height: 101px;
}

.collect-intro .collect-intro__btn_collects {
    width: 178px;
    height: 105px;
}

.building-stats-list {
    display: flex;
    padding: 30rpx;
    width: 100%;
    transform: translateX(-30rpx);
}

.building-stats-item {
    display: inline-block;
    margin-right: 20rpx;
    width: 80px;
    border-radius: 4rpx;
    background-image: linear-gradient(#ffc55c,#fd9a29);
    vertical-align: top;
}

.menu-nav-list-skeleton {
    display: flex;
    flex-wrap: wrap;
    padding: 30rpx 30rpx 0;
}

.menu-nav-item-skeleton {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    width: 20%;
    height: 125rpx;
}

.menu-nav-list {
    display: flex;
    flex-wrap: wrap;
    padding: 30rpx 0 0;
}

.module-guide-container {
    position: relative;
    padding-top: 0;
}

.menu-nav-item {
    margin-bottom: 25rpx;
    width: 20%;
    height: 125rpx;
}

.menu-nav-info {
    flex-direction: column;
    width: 100%;
    height: 100%;
}

.menu-nav-icon,.menu-nav-info {
    display: flex;
    justify-content: center;
    align-items: center;
}

.menu-nav-icon {
    width: 50rpx;
    width: 100%;
    height: 50rpx;
    font-size: 30rpx;
    color: #fff;
}

.menu-nav-icon_small {
    width: 106rpx;
}

.menu-nav-icon_circle {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.icon_nav {
    width: 56rpx;
    height: 56rpx;
}

.icon_nav_small {
    width: 106rpx;
    height: 48rpx;
}

.menu-nav-name {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 14rpx;
    font-size: 24rpx;
    color: #333;
}

.article-info {
    padding: 42rpx 30rpx 0;
}

.article-info-title {
    display: flex;
}

.article-info-title__tip {
    flex: 1;
    font-size: 38rpx;
    color: #333;
    font-weight: 700;
}

.look-more-articles {
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #999;
}

.icon_arrow {
    width: 24rpx;
    height: 32rpx;
}

.article-info-item {
    padding: 30rpx 0;
    border-bottom: 1px solid #ededed;
}

.article-info-footer {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 14rpx;
    font-size: 24rpx;
    color: #999;
}

.building-card-info {
    padding: 0 28rpx 0 30rpx;
    border-bottom: 14rpx solid #f7f7f7;
}

.building-card-title {
    display: flex;
    margin: 32rpx 0;
}

.building-card-title__tip {
    font-size: 38rpx;
    color: #333;
    font-weight: 700;
    display: flex;
    align-items: center;
}

.popularity-icon {
    width: 50rpx;
    height: 40rpx;
    margin-left: 4rpx;
}

.broker-info {
    padding: 30rpx 30rpx 35rpx;
    border-bottom: 14rpx solid #f7f7f7;
}

.broker-info-title {
    display: flex;
    align-items: center;
    font-size: 38rpx;
    color: #333;
    font-weight: 700;
    padding: 10rpx 0;
}

.broker-card-list {
    margin-top: 30rpx;
    width: 100%;
    display: flex;
}

.location-dialog {
    position: fixed;
    top: 50%;
    left: 50%;
    width: 650rpx;
    background-color: #fff;
    border-radius: 8rpx;
    transform: translate(-50%,-50%);
    z-index: 5;
}

.location-dialog__header {
    padding-left: 30rpx;
    height: 77rpx;
    border-bottom: 1px solid #ededed;
    font-size: 28rpx;
}

.location-dialog__body,.location-dialog__header {
    display: flex;
    align-items: center;
    color: #333;
}

.location-dialog__body {
    justify-content: center;
    height: 199rpx;
    font-size: 38rpx;
    font-weight: 500;
    font-size: 36rpx;
}

.location-dialog__footer {
    display: flex;
    height: 90rpx;
    border-top: 1px solid #ededed;
}

.location-btn_cancel,.location-btn_confirm {
    display: flex;
    justify-content: center;
    align-items: center;
    flex: 1;
    color: #fe5e10;
}

.location-btn_cancel {
    border-right: 1px solid #ededed;
    color: #979797;
}

.location-mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #414141;
    opacity: .84;
}

::-webkit-scrollbar {
    display: none;
    width: 0;
    height: 0;
    color: transparent;
}

.new-guide {
    position: fixed;
    left: 0;
    top: 0;
    width: 100vw;
    height: 100vh;
    background: rgba(0,0,0,.6);
    z-index: 2;
}

.new-guide-common {
    position: absolute;
    z-index: 3;
}

.new-guide-1 {
    top: 10%;
    right: 14%;
    width: 420rpx;
    height: 130rpx;
}

.new-guide-2 {
    top: -52%;
    left: 1%;
    width: 162rpx;
    height: 272rpx;
}

.new-guide-4 {
    width: 170rpx;
    height: 198rpx;
    top: -27%;
    left: 21%;
}

.new-guide-5 {
    width: 154rpx;
    height: 266rpx;
    top: 54%;
    left: 20%;
}

.new-guide-6 {
    width: 174rpx;
    height: 276rpx;
    top: 4%;
    right: 19%;
}

.new-guide-3 {
    bottom: 6%;
    right: 10%;
    width: 226rpx;
    height: 86rpx;
}

.mgr-16 {
    margin-right: 16rpx;
}

.is-reactom {
    padding: 20rpx 0 24rpx;
    text-align: center;
    font-size: 24rpx;
    color: #999;
}