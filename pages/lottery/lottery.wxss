./style/detail_panel.wxssrpx.lottery-bg {
    width: 750rpx;
    height: 100vh;
    position: fixed;
    left: 0;
    top: 0;
    z-index: -1;
    background: linear-gradient(90deg,#ffa104,#ff831c);
}

.detail__panel-title {
    font-size: 34rpx;
}

.detail__panel-body {
    padding-top: 10rpx;
}

page {
    padding: 0 30rpx;
    box-sizing: border-box;
    min-height: 100%;
}

.lottery__form-fixed {
    position: fixed;
    left: 5%;
    width: 90%;
    z-index: 4;
}

.lottery__form__input-wrap {
    display: flex;
    align-items: center;
    box-sizing: border-box;
    padding: 0 16rpx;
    height: 72rpx;
    background: #f4f4f5;
    border-radius: 8rpx;
}

.icon-guanbi {
    width: 32rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    border-radius: 50%;
    background: #777;
    color: #fff;
    font-size: 16rpx;
    position: relative;
    z-index: 4;
    border: 10rpx solid #f4f4f5;
}

.lottery__form__input-icon {
    flex: none;
    width: 28rpx;
    height: 28rpx;
}

.lottery__form__input {
    flex: 1;
    padding-left: 14rpx;
    font-size: 15px;
    color: #060606;
}

.lottery__result-title {
    color: #777;
    font-size: 14px;
    line-height: 40rpx;
    margin-bottom: 40rpx;
}

.lottery__result-list {
    margin: 0 auto;
    width: 690rpx;
}

.lottery__result-toast {
    font-size: 30rpx;
    color: rgba(0,0,0,.33);
    line-height: 42rpx;
    text-align: center;
    margin-top: 49rpx;
}

.no-search {
    text-align: center;
}

.no-search-tip {
    font-size: 28rpx;
    color: #999;
    line-height: 40rpx;
    margin-top: 31rpx;
}

.no-sub {
    width: 282rpx;
    height: 283rpx;
}

.lottery__result-empty {
    height: 828rpx;
    background: #fff;
    box-shadow: 0 2rpx 28rpx 0 hsla(0,0%,60%,.3);
    border-radius: 8rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.lottery__result-empty image {
    width: 155rpx;
    height: 208rpx;
}

.lottery__result-item {
    position: relative;
    box-sizing: border-box;
    padding: 25rpx;
    margin-bottom: 30rpx;
    background: #fff;
    border-radius: 6rpx;
    overflow: hidden;
}

.lottery__result-item .b__cell:not(:last-child) {
    margin-bottom: 8rpx;
}

.lottery__result-icon {
    float: left;
    width: 30rpx;
    height: 30rpx;
    padding-top: 6rpx;
}

.lottery__result-icon image {
    display: block;
    width: 100%;
    height: 100%;
}

.lottery__result-content {
    margin-left: 50rpx;
}

.lottery__result-project {
    position: relative;
    height: 42rpx;
    line-height: 42rpx;
    font-size: 16px;
    font-weight: 700;
    padding-bottom: 15rpx;
    border-bottom: 1px solid #ededed;
    margin-bottom: 25rpx;
}

.lottery__result-project .name {
    display: block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.lottery__result-project .name.short {
    max-width: 360rpx;
}

.lottery__result-project .desc {
    position: absolute;
    top: 0;
    right: 0;
    height: 42rpx;
    line-height: 42rpx;
    font-size: 13px;
    color: #8e8f91;
    padding: 0;
    background: transparent;
}

.lottery__result-project .desc.orange {
    color: #ff7504;
}

.lottery__result-nomore {
    font-size: 24rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #d0d0d0;
}

.popup-body .b__cell:not(:last-child) {
    margin-bottom: 28rpx;
}

.popup-body .b__cell-title {
    text-align: right;
    font-size: 28rpx;
    width: 170rpx;
}

.popup-body .b__cell-content {
    margin-left: 190rpx;
    font-size: 30rpx;
}

.popup-body .b__cell-content .input {
    width: 400rpx;
    height: 36px;
    position: relative;
    top: -8px;
}

.popup-body .b__cell-content .input input {
    display: block;
    width: 100%;
    height: 100%;
    background: #f9f9f9;
    border-radius: 4rpx;
    box-sizing: border-box;
    padding-left: 20rpx;
    font-size: 16px;
    font-weight: 700;
}

.b__cell-title {
    width: 240rpx;
    text-align: right;
    box-sizing: border-box;
    padding-right: 5px;
}

.b__cell-content {
    margin-left: 210rpx;
}

.lottery__cell-wrap {
    position: relative;
}

.formid__btn {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    color: transparent;
}

.lottery__cell-arrow {
    position: absolute;
    width: 14rpx;
    height: 32rpx;
    top: 50%;
    right: 20rpx;
    margin-top: -16rpx;
}

.data-table {
    color: #333;
    background: #fff;
    border-radius: 4rpx;
    overflow: hidden;
    border-left: 1rpx solid #fffaf8;
    border-top: 1rpx solid #fffaf8;
}

.data-row {
    display: flex;
    align-items: center;
}

.data-row.row-head {
    height: 66rpx;
    background: #f4f4f9;
}

.data-cell {
    flex: 1;
    text-align: center;
    font-size: 26rpx;
    height: 66rpx;
    border-right: 1rpx solid #fffaf8;
    border-bottom: 1rpx solid #fffaf8;
    line-height: 66rpx;
    color: #333;
}

.data-row.row-head .data-cell {
    color: #88756c;
    font-weight: 500;
}

.data-cell.cell-title {
    color: #888;
}

.data-cell.cell-orange {
    color: red;
}

.calc-img {
    display: block;
    width: 666rpx;
    height: 272rpx;
    margin: 0 auto;
    transition: all .2s;
}

.calc-img.hidden {
    height: 0;
}

.detail__tips {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    padding-left: 29rpx;
    height: 62rpx;
    line-height: 62rpx;
    background: #fff4e5;
    font-size: 20rpx;
    color: #fe2d17;
}

.nav__lottery-head {
    position: relative;
}

.nav__lottery-back {
    position: absolute;
    width: 46rpx;
    height: 46rpx;
    top: 60rpx;
    left: 35rpx;
}

.nav__lottery-info {
    width: 100%;
    box-sizing: border-box;
    padding-top: 41rpx;
    display: flex;
}

.nav__lottery-search {
    margin: 30rpx auto;
    display: flex;
    align-items: center;
}

.search-btn {
    padding: 24rpx 7rpx 24rpx 30rpx;
    font-size: 32rpx;
    color: #fff;
    line-height: 32rpx;
}

.nav__lottery-search>.lottery__form {
    box-sizing: border-box;
    height: 80rpx;
    flex: 1;
}

.nav__lottery-poster-wrap {
    width: 223rpx;
    height: 168rpx;
    flex-shrink: 0;
    border-radius: 6rpx;
    background: #f1f1f1;
    position: relative;
}

.nav__lottery-poster-tip {
    position: absolute;
    left: 10rpx;
    top: 10rpx;
    font-size: 20rpx;
    font-weight: 500;
    color: #fff;
    line-height: 28rpx;
    padding: 3rpx 10rpx;
    background-color: #f4b834;
    opacity: .82;
}

.nav__lottery-poster {
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
}

.nav__lottery-name {
    font-size: 38rpx;
    color: #fff;
    line-height: 53rpx;
    box-sizing: border-box;
    padding-left: 30rpx;
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    height: 106rpx;
}

.lottery__info {
    width: 480rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    margin: 31rpx auto;
    background: linear-gradient(180deg,#fff,#fff4ea 89%,#ffcea5);
    box-shadow: 0 2rpx 26rpx 0 rgba(225,74,0,.38);
    border-radius: 45rpx;
    position: relative;
}

.middle__content {
    text-align: center;
    padding: 1rpx;
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    box-sizing: border-box;
    background: linear-gradient(1turn,#f3856b,#ff6853);
    z-index: 5;
}

.middle__img {
    width: 509rpx;
    height: 333rpx;
    margin-top: 211rpx;
}

.input__wrap {
    width: 690rpx;
    background: #fffeff;
    box-shadow: 0 2rpx 12rpx 0 rgba(187,0,0,.37);
    border-radius: 57rpx;
    height: 114rpx;
    display: flex;
    margin: 85rpx auto 0;
    align-items: center;
    justify-content: center;
}

.input__icon {
    width: 28rpx;
    height: 28rpx;
}

.input__text {
    font-size: 30rpx;
    color: #8e8e93;
    padding-left: 14rpx;
    width: 450rpx;
    text-align: left;
}

.lottery__subscribe {
    display: inline-block;
    background: #faac18;
    width: 195rpx;
    height: 50rpx;
    border-radius: 4rpx;
    display: -webkit-flex;
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    align-items: center;
    padding: 0 10rpx;
    margin-top: 8rpx;
}

.lottery__subscribe-cancel {
    display: inline-block;
    font-size: 30rpx;
    color: #faac18;
    padding-top: 12rpx;
}

.lottery__subscribe-icon {
    width: 34rpx;
    height: 34rpx;
}

.lottery__subscribe-text {
    font-size: 26rpx;
    color: #fff;
}

.submask__item-wrap {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 22rpx;
}

.submask__title {
    color: #333;
    font-size: 34rpx;
    padding-bottom: 21rpx;
    line-height: 48rpx;
}

.popup-lotterying .btn-other {
    font-size: 28rpx;
    line-height: 40rpx;
    text-align: center;
    color: #4b70c4;
    background: transparent;
    text-decoration: underline;
    margin: 0;
    padding: 0 0 0 30rpx;
}

.popup-lotterying .phone,.popup-lotterying input {
    background: #f9f9f9;
    font-size: #333333;
    padding: 14rpx 20rpx;
    border-radius: 4rpx;
}

.popup-lotterying .phone,.popup-lotterying .phone-input {
    flex: 1;
}

.save-btn {
    display: flex;
    align-items: center;
    height: 90rpx;
    position: fixed;
    background: rgba(0,0,0,.6);
    border-radius: 100rpx 2rpx 2rpx 100rpx;
    bottom: 94rpx;
    right: 0;
}

.save-icon {
    width: 60rpx;
    height: 60rpx;
    padding-left: 23rpx;
}

.save-text-wrap {
    font-size: 22rpx;
    color: #faf8f9;
    line-height: 1.4;
    padding: 0 20rpx 0 10rpx;
}