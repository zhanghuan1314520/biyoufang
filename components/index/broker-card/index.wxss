.reset-btn {
    background-color: #fff;
    margin: 0;
    padding: 0;
    line-height: 1;
    border-radius: 0;
}

.broker-card-wrapper {
    position: relative;
    width: 220rpx;
    border-radius: 8rpx;
    background: url("data:image/png;base64,") no-repeat,linear-gradient(0,#f9e1c3,#f9efe1);
    background-size: 100%;
    padding-top: 32rpx;
    overflow: hidden;
}

.broker-card-wrapper .area-name {
    font-size: 20rpx;
    color: #915a32;
    position: absolute;
    top: 20rpx;
    left: 9rpx;
}

.broker-card-wrapper .broker-avatar {
    display: flex;
    justify-content: center;
    padding-bottom: 17rpx;
}

.broker-card-wrapper .broker-avatar .broker-avatar__img {
    width: 104rpx;
    height: 104rpx;
    border-radius: 50%;
    background-color: #e6e6e6;
}

.broker-card-wrapper .broker-name {
    font-size: 28rpx;
    color: #915a32;
    padding-bottom: 13rpx;
    text-align: center;
    font-weight: 700;
}

.broker-card-wrapper .building-name {
    font-size: 20rpx;
    color: #915a32;
    padding-bottom: 14rpx;
    text-align: center;
}

.broker-card-wrapper .advisory {
    display: flex;
    justify-content: center;
    padding-bottom: 18rpx;
}

.broker-card-wrapper .advisory .advisory-btn {
    font-size: 22rpx;
    color: #ff9800;
    position: relative;
    width: 140rpx;
    height: 48rpx;
    line-height: 48rpx;
    background: url("data:image/png;base64,") no-repeat 26rpx;
    background-size: 32rpx 28rpx;
    font-weight: 700;
    padding-left: 69rpx;
    text-align: left;
    background-color: #fdfcf8;
    border-radius: 25rpx;
}