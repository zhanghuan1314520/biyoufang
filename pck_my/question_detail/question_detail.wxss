page {
    box-sizing: border-box;
    padding: 22rpx 30rpx 0;
}

.title {
    font-size: 40rpx;
    color: #333;
    font-weight: 700;
    padding-bottom: 49rpx;
}

.feedback__wrap {
    margin-top: 90rpx;
    text-align: center;
    padding: 22rpx 0 70rpx;
    border-top: 1rpx solid #ededed;
}

.feedback__text {
    font-size: 32rpx;
    color: #333;
    font-weight: 700;
    line-height: 45rpx;
    padding-bottom: 23rpx;
}

.feedback__btn {
    width: 120rpx;
    height: 120rpx;
    transition: all .2s;
    background: transparent;
}

.feedback__icon {
    width: 100%;
    height: 100%;
}

.main {
    width: 100%;
    overflow: hidden;
}