.nav {
    box-shadow: 0 4rpx 8rpx 0 hsla(0,0%,87.1%,.5);
    padding-top: 65rpx;
    display: flex;
    justify-content: space-around;
    background: #fff;
    position: relative;
    z-index: 2;
    position: -webkit-sticky;
    position: sticky;
}

.nav__item {
    text-align: center;
    line-height: 0;
}

.nav__text {
    padding-bottom: 22rpx;
    font-size: 32rpx;
    color: #8a969e;
}

.nav__bar {
    height: 7rpx;
    background: #fe5e10;
    border-radius: 4rpx;
    display: inline-block;
    width: 0;
    transition: all .3s;
}

.nav-active .nav__text {
    color: #fe660b;
}

.nav-active .nav__bar {
    width: 46rpx;
}

.data__empty {
    background: #fff;
}

.data__empty-imgUrl {
    width: 252rpx;
    height: 252rpx;
    padding-top: 300rpx;
}

.data__empty>view:nth-of-type(1) {
    font-size: 28rpx;
    color: #a1a1a1;
    line-height: 50rpx;
}

.data__empty>view:nth-of-type(2) {
    font-size: 28rpx;
    color: #fe670b;
    line-height: 50rpx;
}

.subscription__item {
    background: #fff;
}

.item__empty {
    background: #f7f7f7;
}

.lottery__item {
    box-sizing: border-box;
    padding: 32rpx 29rpx;
}

.item__top {
    width: 100%;
    display: flex;
}

.item__left,.poster__wrap {
    position: relative;
}

.poster__wrap {
    width: 192rpx;
    height: 144rpx;
    background: #eee;
    border-radius: 4px;
}

.item__status {
    position: absolute;
    top: 4rpx;
    left: 4rpx;
    border-radius: 2rpx;
    font-weight: 700;
    font-size: 24rpx;
    padding: 6rpx 10rpx;
    background: linear-gradient(1turn,#f3856b,#ff6853);
    color: #fff;
    line-height: 24rpx;
}

.poster {
    width: 100%;
    height: 100%;
}

.item__tag {
    position: absolute;
    left: 0;
    top: 0;
    width: 80rpx;
    font-size: 20rpx;
    color: #fff;
    padding: 4rpx 10rpx;
    background: rgba(48,48,48,.7);
}

.item__right {
    flex: 1;
    height: 144rpx;
    box-sizing: border-box;
    padding-left: 20rpx;
}

.item__name {
    color: #333;
    font-size: 30rpx;
    font-weight: 700;
    width: 100%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.item__status-toLottery {
    color: #fe6b55;
    background: #ffeeec;
}

.item__isSubscribe,.item__time {
    font-size: 22rpx;
    color: #8a969e;
}

.item__time {
    margin-top: 3px;
}

.item__no {
    margin-top: 6rpx;
    font-size: 22rpx;
    color: #8a969e;
}

.item__isSubscribe {
    display: flex;
    justify-content: space-between;
    align-self: flex-end;
}

.subscribe__phone-wrap {
    display: flex;
    align-items: flex-end;
}

.subscribe__cancel {
    color: #faac18;
    font-size: 26rpx;
}

.time {
    color: #333;
}

.no,.time {
    padding-left: 6rpx;
}

.empty-wrap {
    width: 278rpx;
    height: 311rpx;
    margin: 280rpx auto 0;
}

.empty-img {
    display: block;
    width: 100%;
    height: 100%;
}

.popup-panel,.popup-subscribe {
    top: 385rpx;
}

.building-list {
    display: flex;
    flex-direction: column;
}

.building-item {
    padding: 30rpx 30rpx 16rpx;
    border-bottom: 1px solid #ededed;
}

.lottery-list {
    display: flex;
    flex-direction: column;
}

.lottery-item {
    margin: 0 30rpx 30rpx;
    padding: 0 40rpx 15rpx 20rpx;
    box-shadow: 0 2rpx 28rpx 0 hsla(0,0%,60%,.3);
}

.lottery-item:nth-of-type(1) {
    margin-top: 40rpx;
}

.pop-up {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,.45);
    z-index: 3;
    padding: 0 40rpx;
}

.pop-up-main {
    background: #fff;
    border-radius: 8rpx;
    margin-top: 500rpx;
    font-size: 38rpx;
    color: #333;
}

.pop-up-head {
    padding-left: 40rpx;
    font-weight: 600;
    line-height: 98rpx;
    border-bottom: 1rpx solid #ededed;
}

.pop-up-text {
    padding: 40rpx;
    line-height: 50rpx;
}

.pop-up-text_lite {
    color: #ff831c;
}

.pop-up-btn {
    padding: 0 40rpx 30rpx;
}

.pop-up-btn>view {
    width: 260rpx;
    height: 76rpx;
    border-radius: 4rpx;
    text-align: center;
    line-height: 76rpx;
}

.pop-up-btn_none {
    background: #ededed;
    color: #999;
}

.pop-up-btn_yes {
    background: linear-gradient(270deg,#fe5e10,#ff831c);
    color: #fff;
}

.nomore {
    border: 0;
}